<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="singer" class = "com.worldexplorers.springioc.bean.Singer">
		<property name="name" value = "Martina"/><!-- setName(String name) -->
		<property name="age" value = "27"/>
		<property name="height" value = "1.9"/>
		<!-- reference property initialization -->
		<property name="school" ref = "school"/><!-- setSchool(School school) -->
	</bean>
</beans>


